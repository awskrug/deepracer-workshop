{
  "name": "deepracer-workshop",
  "version": "1.0.0",
  "homepage": "https://github.com/nalbam/deepracer-workshop",
  "bugs": {
    "url": "https://github.com/nalbam/deepracer-workshop/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nalbam/deepracer-workshop.git"
  },
  "scripts": {
    "theme": "git submodule init && git submodule update",
    "build": "rm -rf docs && hugo -v -b /deepracer-workshop/ -d docs --minify",
    "start": "hugo server -w -v --enableGitInfo --bind=0.0.0.0 --port 8080 --navigateToChanged",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.4.2",
    "hugo-bin": "^0.115.0",
    "mermaid": "^10.4.0"
  },
  "devDependencies": {
    "gh-pages": "^6.0.0"
  }
}
